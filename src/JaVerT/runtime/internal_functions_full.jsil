	gen:	goto [d = "g"] test rf;
	test:	v := l-nth (desc, 3);
			w := l-nth (desc, 4);
			goto [(v = empty) and (w = empty)] rf rt;
	rt:		xret := true;
			goto rlab;
	rf:		xret := false;
	rlab:	skip
