(* *******************
   *** i__toNumber ***
   ******************* *)

spec i__toNumber (v)

	[[ v == true ]]
	[[ types(ret : Num) * (ret == 1) ]]
	normal;

	[[ v == false ]]
	[[ types(ret : Num) * (ret == 0) ]]
	normal;

	[[ (v == #v) * types(#v : Num) ]]
	[[ ret == #v ]]
	normal;

	[[ (v == #v) * types(#v : Str) ]]
	[[ types(ret : Num, #v: Str) *
	   (ret == (string_to_num #v)) ]]
	normal

proc i__toNumber (v) {
    iu:		goto [v = undefined] undef in;
	undef:	xret := nan;
    		goto rlab;

    in:		goto [v = null] null ib;
	null:	xret := 0;
    		goto rlab;

    ib:		goto [typeOf(v) = Bool] bool inum;
	bool:	goto [v = true] is_t is_f;
    is_t:	xret := 1;
    		goto rlab;
    is_f:	xret := 0;
    		goto rlab;

    inum:	goto [typeOf(v) = Num] num is;
    num:	xret := v;
    		goto rlab;

    is:		goto [typeOf(v) = Str] str io;
    str:	xret := string_to_num v;
    		goto rlab;

    io:		goto [typeOf(v) = Obj] obj oops;
    obj:	xret := "i__toPrimitive" (v, "Number") with elab;
    		xret := "i__toNumber" (xret);
    		goto rlab;

    oops:	xret := "TypeError" ("ToNumber: This should not happen.");
            goto elab;

    rlab:	skip;
    elab:	skip
}
with
{
	ret:	xret, rlab;
	err:	xret, elab;
};


