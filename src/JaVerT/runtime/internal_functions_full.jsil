	   ((#l, "@class") -> #cls) * 
	   isClass(#cls, "String") * Cell_s (#l, #prop, "None", None, #pv) *
	   ((#l, "@extensible") -> false) ]]
    [[ ((#l, "@class") -> #cls) * 
	   Cell_s (#l, #prop, "None", None, #pv) * ((#l, "@extensible") -> false) *
	   isTypeError(err) ]]
    error;

    (* 04 - Current is undefined, object is not extensible, string, don't throw *)
    [[ (l == #l) * (prop == #prop) * (desc == #desc) * (throw == false) *
