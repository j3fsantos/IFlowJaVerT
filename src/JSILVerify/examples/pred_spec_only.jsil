pred factorial (n, f) :
  (n == 0) * (f == 1),
	(!(n <# 1)) * (#n1 == (n - 1)) * factorial(#n1, #f1) * (f == #f) * (f == (n * #f1));

pred dog (x, y) :
  (x == 1) * (y <=# 5),
  (x == 2) * (! y <=# 5);

