(* Test similar property as test_append_01 but over Strings *)
only spec subString(l, i, j)
	[[	(l == #l1 ++ (#l2 ++ #l3)) * (i == #i) * (j == #j) * (s-len(#l1) == #i) * (s-len(#l2) == #j) * 
		types(l : Str, #l1 : Str, #l2 : Str, #l3 : Str, #i : Num, #j : Num) ]]
	[[ ret == #l2 ]]
	normal

spec test_01 ()
	[[ emp ]]
	[[ (ret == "ef") * types(ret : Str) ]]
	normal

proc test_01 ()
{
			string := "abcd" ++ "efgh";
			xret := "subString" (string, 4, 2);
	rlab:	skip
}
with
{
	ret:	xret, rlab;
};