spec sets_01 ()
	[[ emp ]]
	[[ (ret == $$t) * types(ret : $$boolean_type) ]]
	normal

proc sets_01 ()
{
			xret := -{- 1, 2, 3 -}-;
			xret := (xret -u- -{- "a", "b", "c" -}-);
			xret := (-{- "a", 2, "d" -}- -i- xret);
			xret := (xret -d- -{- 2 -}-);
			yret := (xret -s- -{- 5, 6, $$t, "a" -}-);
			xret := (yret and (not ("b" -e- xret)));
	rlab:	skip
}
with
{
	ret:	xret, rlab;
};

spec sets_02 (x, y)
	[[ (x == #x) * (y == #y) * types (#x : $$number_type, #y : $$number_type) ]]
	[[ (ret == $$t) * types(ret : $$boolean_type) ]]
	normal

proc sets_02 (x, y)
{
			seti := -{- x, y -}-;
			setd := seti -d- -{- x -}-;
			xret := not (setd = -{- -}-);
	rlab:	skip
}
with
{
	ret:	xret, rlab;
};

spec sets_03 (x, y)
	[[ (x == #x) * (y == #y) * types (#x : $$number_type, #y : $$number_type) ]]
	[[ (ret == $$f) * types(ret : $$boolean_type) ]]
	normal

proc sets_03 (x, y)
{
			seti := -{- x, y -}-;
			setd := seti -d- -{- x -}-;
			xret := not (setd = -{- -}-);
	rlab:	skip
}
with
{
	ret:	xret, rlab;
};