   [[ Map (m, -u- (-{ {{ #key, #value_old }} }-, #rkvs)) * (v == #value_new) ]]
   [[ Map (m, -u- (-{ {{ #key, #value_new }} }-, #rkvs)) * (ret == #value_new) ]]
   normal
