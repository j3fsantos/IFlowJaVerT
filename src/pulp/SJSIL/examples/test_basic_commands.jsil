(** Tested feature: field assignment 1               **)
(** Expected result: YES                             **)
spec test0 (x)
   [[ emp ]]
   [[ ((x, "cenoura") -> "maria") * ((x, "@proto") -> $$null) ]]
   normal
proc test0 (x) {
  		   x := new ();
           [x, "cenoura"] := "maria";
   rlab:   skip
} with { ret: x, rlab; };


