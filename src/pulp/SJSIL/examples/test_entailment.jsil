
(** Check if (#x == "maria") entails (! #x == None) **)
(** Expected result: YES                            **)
spec test1 (x)
   [[ (x == #x) * (#x == "maria") * types(#x: $$string_type) ]]
   [[ (! (x == None)) ]]
   normal
proc test1 (x) {
   rlab:   skip
} with { ret: x, rlab; };


