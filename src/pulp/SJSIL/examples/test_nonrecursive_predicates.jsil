pred p1 (x, y) : emp,
                 x == y;
pred p1 ("hello", x) : x == "hi";
pred p1 (a, None) : (0 == a) * p3(a);
pred p2 (e, 5) : (e <# 10) * p1(e, #x);
pred p3 (a) : (a <=# #b) * p4(#b) ;
pred p4 (a) : p3(a) ;

spec xpto (x)
   [[ ((x - 3) <=# 0) * types(x: $$int_type) ]]
   [[ p1("hello", "ho") ]]
   normal
proc xpto (x) {
			skip;
			[* fold p1("hello", "ho") *]
	rlab:   skip
}
with
{
	ret: 	x, rlab;
};
