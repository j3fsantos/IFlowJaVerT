import "field_logic";
import "list_logic";
import "le_lt_logic";
import "pulp_footprint_logic";
import "proto_field_logic";
import "pulp_type_logic";
  
rule field_loc:
  | field(?x,?f,?t) |-  field(?z,?f,?t) 
without
  ?x!=?z
if
  field(?x,?f,?t) | |- ?x=?z 
  
constructor numeric_const
constructor undefined
constructor null
constructor none
constructor lg