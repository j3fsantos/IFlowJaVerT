only spec code_free_proc (x)

	[[ (x == #x) * types (#x : $$number_type) ]]
	[[ (ret == #x + 1) * types (ret : $$number_type, #x : $$number_type) ]]
	normal

spec do_things_because_why_not (x)

	[[ (x == #x) * types (#x : $$number_type) ]]
	[[ (ret == 2 * (#x + 1)) * types (ret : $$number_type, #x : $$number_type) ]]
	normal
	
proc do_things_because_why_not (x) {
			
			xcfp := "code_free_proc" (x);
			xret := xcfp * 2;

	rlab:	skip
}
with
{
	ret: 	xret, rlab;
};